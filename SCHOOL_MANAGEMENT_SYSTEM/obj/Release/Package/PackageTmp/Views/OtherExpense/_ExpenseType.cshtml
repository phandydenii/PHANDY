<div class="modal fade" data-backdrop="static" data-keyboard="fale" id="expensetypeModel" tabindex="-1" role="dialog" aria-labelledby="expensetypeModel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div id="loadingGif"></div>
            <div class="modal-header">
                <button type="button" onclick="CloseExpenseType();" class="btn-danger btn-xs pull-right" data-dismiss="modal" aria-hidden="true"><span class="glyphicon glyphicon-remove"></span></button>
                <b class="modal-title" style="color:darkblue">@Resources.Content.ManageExpenseType</b>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <input type="text" hidden value="" />
                            <input id="idtype" name="idtype" type="text" value="" class="form-control input-sm hidden" placeholder="Enter ID" />
                            <label for="typename">ប្រភេទចំណាយ</label>
                            <input id="typename" name="typename" type="text" value="" class="form-control input-sm" placeholder="Enter Category Name" />
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <button type="button" id="btnExpenseType" onclick="ExpenseTypeAction();" class="btn btn-primary btn-block btn-sm">Add New</button>
                    </div>
                </div>

                <hr />

                <div class="row">
                    <div class="col-md-12">
                        <table id="expensetypeTable" class="table-hover table-bordered table-condensed table-striped" style="width:100%">
                            <thead>
                                <tr>
                                    @*<td>Id</td>*@
                                    <td>ប្រភេទចំណាយ</td>
                                    <td>សកម្មភាព</td>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



