<div class="modal fade" data-backdrop="static" data-keyboard="fale" id="studyperiodModel" tabindex="-1" role="dialog" aria-labelledby="studyperiodModel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div id="loadingGif"></div>
            <div class="modal-header">
                <button type="button" class="btn-danger btn-xs pull-right" data-dismiss="modal" aria-hidden="true">Close</button>
                <b class="modal-title" style="color:darkblue">Manage Study Period</b>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <input type="text" hidden value="" />
                            <input id="studyperiodid" name="studyperiodid" type="text" value="" class="form-control input-sm hidden" placeholder="Enter Studyperiodid" />
                            <label for="period">Period</label>
                            <input id="period" name="period" type="text" value="" class="form-control input-sm" placeholder="Enter Period" />
                        </div>
                        <div class="form-group">
                            <label for="adminfee">Admin Fee</label>
                            <input id="adminfee" name="adminfee" type="text" value="" class="form-control input-sm" placeholder="Enter Admin Fee" />
                        </div>
                        <div class="form-group">
                            <label for="periodstatus">Status</label>
                            <input id="periodstatus" name="periodstatus" type="text" value="" class="form-control input-sm hidden" placeholder="Enter Status" />
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <button type="button" id="btnPeriod" onclick="PeriodAction();" class="btn btn-primary btn-block btn-sm">Add New</button>
                    </div>
                </div>

                <hr />

                <div class="row">
                    <div class="col-md-12">
                        <table id="periodTable" class="table-hover table-bordered table-condensed table-striped" style="width:100%">
                            <thead>
                                <tr>
                                    @*<td>Id</td>*@
                                    <td>Period</td>
                                    <td>Admin Fee</td>
                                    <td>Status</td>
                                    <td>Action</td>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



