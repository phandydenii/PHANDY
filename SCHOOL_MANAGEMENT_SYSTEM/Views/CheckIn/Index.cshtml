
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid">
    <div class="row">
        <div class="col-md-6">
            <h4 style="color: darkblue;"><b>@Resources.Content.ManageCheckIn</b></h4>
        </div>
    </div>
    <hr />
    <table class="table-hover table-bordered table-condensed table-striped" style="width:100%" id="tableCheckInDetail">
        <thead>
            <tr>
                <th>@Resources.Content.No</th>
                <th>@Resources.Content.Name</th>
                <th>@Resources.Content.CheckInDate</th>
                <th>@Resources.Content.RoomNo</th>
                <th>@Resources.Content.RoomType</th>
                <th>@Resources.Content.PayForRoom</th>
                <th>@Resources.Content.PrePaid</th>
                <th>@Resources.Content.Man</th>
                <th>@Resources.Content.Woman</th>
                <th>@Resources.Content.Child</th>
                @*<th>ស្ថានភាព</th>*@
                <th style="width:12%">@Resources.Content.Action</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>

@Html.Partial("_CheckIn")
@Html.Partial("_CheckOut")
@Html.Partial("_History")


@section scripts{
    @Scripts.Render("~/Scripts/Functions/checkin.js")
    @Scripts.Render("~/Scripts/moment.min.js")
    @*@Scripts.Render("~/Scripts/Functions/history.js")*@
}

