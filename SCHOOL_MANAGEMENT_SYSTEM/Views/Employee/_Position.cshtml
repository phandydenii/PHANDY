
<div class="modal fade" data-backdrop="static" data-keyboard="fale" id="positionModel" tabindex="-1" role="dialog" aria-labelledby="positionModel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div id="loadingGif"></div>
            <div class="modal-header">
                <button type="button" class="btn-danger btn-xs pull-right" data-dismiss="modal" aria-hidden="true">Close</button>
                <b class="modal-title" style="color:darkblue">ការគ្រប់គ្រង មុខដំណែង</b>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <input type="text" hidden value="" />
                            <input id="positionid" name="positionid" type="text" value="" class="form-control input-sm hidden" placeholder="Enter Positionid" />
                            <label for="positionname">មុខដំណែង</label>
                            <input id="positionname" name="positionname" type="text" value="" class="form-control input-sm" placeholder="Enter Position Name" />
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <button type="button" id="btnPosition" onclick="PositionAction();" class="btn btn-primary btn-block btn-sm">Add New</button>
                    </div>
                </div>

                <hr />

                <div class="row">
                    <div class="col-md-12">
                        <table id="positionTable" class="table-hover table-bordered table-condensed table-striped" style="width:100%">
                            <thead>
                                <tr>
                                    @*<td>Position Id</td>*@
                                    <td>មុខដំណែង</td>
                                    <td>សកម្មភាព</td>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>



            </div>
        </div>
    </div>
</div>


