
    <div class="modal fade" data-backdrop="static" data-keyboard="fale" id="FeedbackModel" tabindex="-1" role="dialog" aria-labelledby="FeedbackModel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div id="loadingGif"></div>
                <div class="modal-header">
                    <button type="button" class="btn-danger btn-xs pull-right" data-dismiss="modal" aria-hidden="true">Close</button>
                    <b class="modal-title" style="color:darkblue">មតិកែលម្អ</b>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <input id="feedbackidtemp" name="feedbackidtemp" type="text" value="0" class="form-control input-sm hidden" placeholder="Enter employeeId" />

                        <div class="col-md-4">
                            <div class="form-group">
                                <img class="center img-responsive img-thumbnail" id="image" src="~/Images/no_image.png" />
                            </div>
                            <div class="form-group">
                                <label>Image</label>
                                <input onchange="readURLFeedback(this);" type="file" id="file" style="width:100%;" />
                                <input type="hidden" value="" id="file_old" />
                            </div>
                        </div>
                        <div class="col-md-8">
                            <input type="text" hidden value="" />
                            <input id="fid" name="fid" type="text" value="" class="form-control input-sm hidden" placeholder="Enter employeeId" />

                            <div class="form-group">
                                <label for="date">Date</label>
                                <input id="date" name="date" type="date" value="" class="form-control input-sm" placeholder="Enter Employee Name" />

                            </div>
                            <div class="form-group">
                                <label for="customerid">Customer</label>
                                <select class="form-control input-sm" id="customerid">
                                    @foreach (var customer in Model.Customers)
                                    {
                                        <option value=@customer.Id>@customer.name</option>
                                    }
                                </select>
                            </div>
                            <div class="form-group">
                                <label for="comment">Comment</label>
                                <input id="comment" name="comment" type="text" value="" class="form-control input-sm" />

                            </div>
                        </div>
                        <hr />
                        <div class="row">
                            <div class="col-md-12">
                                <button type="button" id="btnSaveFeedback" onclick="FeedbackAction();" class="btn btn-primary btn-block btn-sm">Add New</button>
                            </div>
                        </div>
                        <hr />
                        <div class="form-group">
                            <div class="col-md-12">
                                <table id="tableFeedback" class="table-hover table-bordered table-condensed table-striped" style="width:100%">
                                    <thead>
                                        <tr>
                                            <th>កាលបរិច្ឆេទ</th>
                                            <th>អតិថិជន</th>
                                            <th>មតិ</th>
                                            <th>សកម្មភាព</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
