<div class="modal fade" data-backdrop="static" data-keyboard="fale" id="courseModel" tabindex="-1" role="dialog" aria-labelledby="courseModel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div id="loadingGif"></div>
            <div class="modal-header">
                <button type="button" class="btn-danger btn-xs pull-right" data-dismiss="modal" aria-hidden="true">Close</button>
                <b class="modal-title" style="color:darkblue">Manage Course</b>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <input type="text" hidden value="" />
                            <input id="id" name="id" type="text" value="" class="form-control input-sm hidden" placeholder="Enter ID" />
                            <label for="coursecode">Course Code</label>
                            <input id="coursecode" name="coursecode" type="text" value="" class="form-control input-sm" placeholder="Enter Course Code" />
                        </div>
                        <div class="form-group">
                            <label for="coursename">Course Name</label>
                            <input id="coursename" name="coursename" type="text" value="" class="form-control input-sm" placeholder="Enter Course Name" />
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="coursenamekh">Course Name in Khmer</label>
                            <input id="coursenamekh" name="coursenamekh" type="text" value="" class="form-control input-sm" placeholder="Enter Course Name in Khmer" />
                        </div>
                        <div class="form-group">
                            <label for="tutionfee">Turtion Fee</label>
                            <input id="tutionfee" name="tutionfee" type="number" value="" class="form-control input-sm" placeholder="Enter Turtion Fee" />
                            
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <button type="button" id="btnCourse" onclick="CourseAction();" class="btn btn-primary btn-block btn-sm">Add New</button>
                    </div>
                </div>

                <hr />

                <div class="row">
                    <div class="col-md-12">
                        <table id="courseTable" class="table-hover table-bordered table-condensed table-striped" style="width:100%">
                            <thead>
                                <tr>
                                    @*<td>Id</td>*@
                                    <td>Course Code</td>
                                    <td>Course Name</td>
                                    <td>Course NameKh</td>
                                    <td>Turtion Fee</td>
                                    <td>Action</td>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>




