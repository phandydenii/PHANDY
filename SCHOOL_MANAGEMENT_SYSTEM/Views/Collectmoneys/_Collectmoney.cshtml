<form id="collectmoney" method="post" enctype="multipart/form-data">

    <div class="modal fade" data-backdrop="static" data-keyboard="fale" id="collectmoneyModel" tabindex="-1" role="dialog" aria-labelledby="collectmoneyModel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div id="loadingGif"></div>
                <div class="modal-header">
                    <button type="button" class="btn-danger btn-xs pull-right" data-dismiss="modal" aria-hidden="true">Close</button>
                    <b class="modal-title" style="color:darkblue">គ្រប់គ្រងការប្រមូលប្រាក់ពីអ្នកដឹក</b>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-6">
                            <input type="text" hidden value="" />
                            <input id="id" name="id" type="text" value="" class="form-control input-sm hidden" placeholder="Enter employeeId" />

                            <div class="form-group">
                                <label for="date">ថ្ងៃខែ</label>
                                <input id="date" name="date" type="date" value="" class="form-control input-sm" placeholder="Enter Product Name" />

                            </div>
                            <div class="form-group">
                                <label for="employeeid">អ្នកដឹក</label>
                                <select class="form-control input-sm selectpicker" id="employeeid" data-style="btn-dark" data-hide-disabled="true" data-live-search="true">
                                    @foreach (var stu in Model.Employee)
                                    {
                                        <option value=@stu.Id>@stu.namekh</option>
                                    }
                                </select>
                            </div>
                            <div class="form-group">
                                <label for="amount">ចំនួនប្រាក់</label>
                                <input id="amount" name="amount" type="number" value="0.00" class="form-control input-sm" />

                            </div>
                           
                            <div class="form-group">
                                    <label for="status">បានទូរទាត់រួច</label>
                                    <input id="status" name="status" type="checkbox" value="" class="form-control-new input-sm" />

                            </div>
                            
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="deliveryin">ប្រាក់យក</label>
                                <input id="deliveryin" name="deliveryin" type="text" value="0.00" class="form-control input-sm" />

                            </div>
                            <div class="form-group">
                                <label for="deliveryout">ប្រាក់ដឹក</label>
                                <input id="deliveryout" name="deliveryout" type="text" value="0.00" class="form-control input-sm" />

                            </div>
                            <div class="form-group">
                                <label for="bonus">ប្រាក់បន្ថែម</label>
                                <input id="bonus" name="bonus" type="text" value="0.00" class="form-control input-sm" />

                            </div>
                            <hr />
                            <div class="form-group">
                                <button type="button" id="btnCollectmoney" onclick="CollectMondeyAction();" class="btn btn-primary btn-block btn-sm">Add New</button>

                            </div>
                            
                        </div>
                        
                    </div>

                    <div class="row">
                        <div class="col-md-6">
                            <button type="button" id="btnDeliveryin" onclick="LoadDeliveryin();" class="btn btn-success btn-block btn-sm">Load DeliveryIn</button>
                        </div>
                        <div class="col-md-6">
                            <button type="button" id="btnDeliveryout" onclick="LoadDeliveryout();" class="btn btn-success btn-block btn-sm">Load DeliveryOut</button>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="col-md-12">
                            <table id="deliveryTable" class="table-hover table-bordered table-condensed table-striped" style="width:100%">
                                <thead>
                                    <tr>
                                        <td>ទីតាំង</td>
                                        @*<td>ទំនិញ</td>
                                        <td>អ្នកដឹក</td>*@
                                        <td>លេខទូរស័ព្ទ</td>
                                        <td>តម្លៃ</td>
                                        <td>សេវាឡាន</td>
                                        <td>សេវាដឹក</td>
                                        <td>ប្រភេទដឹក</td>
                                        <td>ស្ថានភាព</td>
                                        @*<td>សកម្មភាព</td>*@
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</form>

