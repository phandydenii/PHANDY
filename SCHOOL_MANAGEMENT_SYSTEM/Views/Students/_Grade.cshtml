<div class="modal fade" data-backdrop="static" data-keyboard="fale" id="gradeModel" tabindex="-1" role="dialog" aria-labelledby="gradeModel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div id="loadingGif"></div>
            <div class="modal-header">
                <button type="button" class="btn-danger btn-xs pull-right" data-dismiss="modal" aria-hidden="true">Close</button>
                <b class="modal-title" style="color:darkblue">Manage Grade</b>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <input type="text" hidden value="" />
                            <input id="gradeid" name="gradeid" type="text" value="" class="form-control input-sm hidden" placeholder="Enter GradeId" />
                            <label for="gradename">Grade Name</label>
                            <input id="gradename" name="gradename" type="text" value="" class="form-control input-sm" placeholder="Enter Grade" />
                        </div>
                        <div class="form-group">
                            <label for="gradestatus">Grade Status</label>
                            <input id="gradestatus" name="gradestatus" type="text" value="" class="form-control input-sm hidden" placeholder="Enter Status" />
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <button type="button" id="btnGrade" onclick="GradeAction();" class="btn btn-primary btn-block btn-sm">Add New</button>
                    </div>
                </div>

                <hr />

                <div class="row">
                    <div class="col-md-12">
                        <table id="gradeTable" class="table-hover table-bordered table-condensed table-striped" style="width:100%">
                            <thead>
                                <tr>
                                    @*<td>Grade Id</td>*@
                                    <td>Grade Name</td>
                                    @*<td>Grade Status</td>*@
                                    <td>Action</td>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


