
@{
    ViewBag.Title = "Manage Other Expense";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div id="loadingGif"></div>
<div class="row">
    <div class="col-md-6">
        <h4 style="color: darkblue;">គ្រប់គ្រងការចូលស្នាក់នៅ</h4>
    </div>
    <div class="col-md-6">
        <a href="#" class="btn btn-success btn-sm pull-right" data-toggle="modal" data-target="#expensetypeModel" style="margin-right:5px;"><span class="glyphicon glyphicon-cog"></span> ប្រភេទចំណាយ</a>
        <a href="#" class="btn btn-success btn-sm pull-right"​​ onclick="ClickAddnewOtherExpense()" data-toggle="modal" data-target="#otherexpenseModel" style="margin-right:5px;"><span class="glyphicon glyphicon-plus-sign"></span> បង្កើតចំណាយថ្មី</a>
    </div>
</div>
<hr />

<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <label for="date">មើលតាមកាលបរិច្ឆេទ:</label>
            <div class="form-inline">
                <label for="fromdate">ចាប់ពីថ្ងៃទី:</label>
                <input id="fromdate" name="fromdate" type="date" value="@DateTime.Now.ToString("yyyy'-'MM'-'dd")" class="form-control input-sm" placeholder="Enter Product Name" />
                <label for="todate">ដល់:</label>
                <input id="todate" name="todate" type="date" value="@DateTime.Now.ToString("yyyy'-'MM'-'dd")" class="form-control input-sm" placeholder="Enter Product Name" />
            </div>
        </div>
    </div>
    <table id="tableOtherExpense" class="table table-hover table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>ថ្ងៃខែ</th>
                <th>ប្រភេទចំណាយ</th>
                <th>ចំនួនប្រាក់</th>
                <th>បរិយាយ</th>
                <th style="width:10%">សកម្មភាព</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>








@Html.Partial("_ExpenseType")
@Html.Partial("_OtherExpense")
@section scripts
{
    
    @Scripts.Render("~/Scripts/Functions/expensetype.js")
    @Scripts.Render("~/Scripts/Functions/otherexpense.js")
    @Scripts.Render("~/Scripts/moment.min.js")
}
