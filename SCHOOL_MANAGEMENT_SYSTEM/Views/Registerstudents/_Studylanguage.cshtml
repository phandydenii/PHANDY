<div class="modal fade" data-backdrop="static" data-keyboard="fale" id="studylanguageModel" tabindex="-1" role="dialog" aria-labelledby="studylanguageModel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div id="loadingGif"></div>
            <div class="modal-header">
                <button type="button" class="btn-danger btn-xs pull-right" data-dismiss="modal" aria-hidden="true">Close</button>
                <b class="modal-title" style="color:darkblue">Manage Study Language</b>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <input type="text" hidden value="" />
                            <input id="studylanguageid" name="studylanguageid" type="text" value="" class="form-control input-sm hidden" placeholder="Enter Studyperiodid" />
                            <label for="language">Language</label>
                            <input id="language" name="language" type="text" value="" class="form-control input-sm" placeholder="Enter Lanuge" />
                        </div>
                        <div class="form-group">
                            <label for="languagestatus">Status</label>
                            <input id="languagestatus" name="languagestatus" type="text" value="" class="form-control input-sm hidden" placeholder="Enter Status" />
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <button type="button" id="btnLanguage" onclick="languageAction();" class="btn btn-primary btn-block btn-sm">Add New</button>
                    </div>
                </div>

                <hr />

                <div class="row">
                    <div class="col-md-12">
                        <table id="languageTable" class="table-hover table-bordered table-condensed table-striped" style="width:100%">
                            <thead>
                                <tr>
                                    @*<td>Id</td>*@
                                    <td>Language</td>
                                    <td>Status</td>
                                    <td>Action</td>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


